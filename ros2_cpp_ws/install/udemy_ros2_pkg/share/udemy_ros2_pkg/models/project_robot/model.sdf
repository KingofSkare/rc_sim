<sdf version="1.9">
    <model name='project_model' canonical_link='chassis'>
        <pose relative_to='world'>0 0 0 0 0 0</pose>   <!--the pose is relative to the world by default-->

        <link name='chassis'>
            <pose relative_to='__model__'>0.5 0 0.4 0 0 0</pose>
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>1.14395</mass>
                <inertia>
                    <ixx>0.126164</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.416519</iyy>
                    <iyz>0</iyz>
                    <izz>0.481014</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <box>
                        <size>2.0 1.0 0.5</size> <!-- this size is in meter-->
                    </box>
                </geometry>
                <!--let's add color to our link-->
                <material>
                    <ambient>0.5 0.0 0.5 1</ambient>
                    <diffuse>0.5 0.0 0.5 1</diffuse>
                    <specular>0.5 0.0 0.5 1</specular>
                </material>
            </visual>
            <collision name='collision'> <!--todo: describe why we need the collision-->
                <geometry>
                    <box>
                        <size>2.0 1.0 0.5</size>
                    </box>
                </geometry>
            </collision>
        </link>

                <!-- front_box -->
        <link name="front_box">
        <pose>2 0.0 0.4  0 0 0</pose> <!-- Plassert i nesen av den opprinnelige linken -->
        <collision name="collision">
            <geometry>
                <box>
                    <size>1.0 0.5 0.5</size>
                </box>
            </geometry>
        </collision>
        <visual name="visual">
            <geometry>
                <box>
                    <size>1 0.5 0.5</size>
                </box>
            </geometry>
        </visual>
        </link>

        <!--let's build the left wheel-->
        <link name='left_wheel_back'>
            <pose relative_to="chassis">-0.5 0.6 0 -1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>1</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
                    <specular>1 1 1 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <!--copy and paste for right wheel but change position-->
        <link name='right_wheel_back'>
            <pose relative_to="chassis">-0.5 -0.6 0 -1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>1</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
                    <specular>1 1 1 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <link name='left_wheel_front'>
            <pose relative_to="front_box">0.25 0.4 0 -1.5707 0 0</pose> 
            <inertial>
                <mass>1</mass>
                <inertia>
                    <ixx>0.1</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.1</iyy>
                    <iyz>0</iyz>
                    <izz>0.1</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                      <ode>
                        <mu>1</mu>
                        <mu2>1.0</mu2>
                      </ode>
                    </friction>
                  </surface>
                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
                    <specular>1 1 1 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <!--copy and paste for right wheel but change position-->
        <link name='right_wheel_front'>
            <pose relative_to="front_box">0.25 -0.4 0 -1.5707 0 0</pose> 
            <inertial>
                <mass>1</mass>
                <inertia>
                    <ixx>0.1</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.1</iyy>
                    <iyz>0</iyz>
                    <izz>0.1</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                      <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                      </ode>
                    </friction>
                  </surface>
                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
                    <specular>1 1 1 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.4</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <link name='left_beam_front'>
            <pose relative_to="front_box">0.25 0.35 0 -1.5707 0 0</pose> 
            <inertial>
                <mass>0.5</mass>
                <inertia>
                    <ixx>0.1</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.1</iyy>
                    <iyz>0</iyz>
                    <izz>0.1</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
                    <specular>1 1 1 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <!--copy and paste for right wheel but change position-->
        <link name='right_beam_front'>
            <pose relative_to="front_box">0.25 -0.35 0 -1.5707 0 0</pose> 
            <inertial>
                <mass>0.5</mass>
                <inertia>
                    <ixx>0.1</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.1</iyy>
                    <iyz>0</iyz>
                    <izz>0.1</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
                    <specular>1 1 1 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.2</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <!-- joint front_box -->
        <!-- Joint som binder de to linkene sammen -->
        <joint name="box_joint" type="fixed">
            <parent>chassis</parent>
            <child>front_box</child>
            <pose>2 0.0 0.4 0 0 0</pose>
        </joint>    


        <!--connecting these links together using joints-->
        <joint name='left_wheel_back_joint' type='revolute'> <!--continous joint is not supported yet-->
            <pose relative_to='left_wheel_back'/>
            <parent>chassis</parent>
            <child>left_wheel_back</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be defined as any frame or even arbitrary frames-->
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                </limit>
            </axis>
        </joint>

        <joint name='right_wheel_back_joint' type='revolute'>
            <pose relative_to='right_wheel_back'/>
            <parent>chassis</parent>
            <child>right_wheel_back</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                </limit>
            </axis>
        </joint>

        <joint name='left_wheel_front_joint' type='revolute'> 
            <pose relative_to='left_wheel_front'/>
            <parent>left_beam_front</parent>
            <child>left_wheel_front</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> 
                <limit>
                    <lower>-1.79769e+308</lower>    
                </limit>
            </axis>
        </joint>

        <joint name='right_wheel_front_joint' type='revolute'>
            <pose relative_to='right_wheel_front'/>
            <parent>right_beam_front</parent>
            <child>right_wheel_front</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>   
                </limit>
            </axis>
        </joint>

        <joint name='left_beam_front_joint' type='fixed'> 
            <pose relative_to='left_beam_front'/>
            <parent>front_box</parent>
            <child>left_beam_front</child>
        </joint>

        <joint name='right_beam_front_joint' type='fixed'>
            <pose relative_to='right_beam_front'/>
            <parent>front_box</parent>
            <child>right_beam_front</child>
        </joint>




        <plugin
            filename="ignition-gazebo-diff-drive-system"
            name="gz::sim::systems::DiffDrive">
            <left_joint>left_wheel_back_joint</left_joint>
            <left_joint>left_wheel_front_joint</left_joint>
            <right_joint>right_wheel_back_joint</right_joint>
            <right_joint>right_wheel_front_joint</right_joint>
            <wheel_separation>1.2</wheel_separation>
            <wheel_radius>0.4</wheel_radius>
            <max_linear_acceleration>2</max_linear_acceleration>
            <min_linear_acceleration>-2</min_linear_acceleration>
            <max_angular_acceleration>4</max_angular_acceleration>
            <min_angular_acceleration>-4</min_angular_acceleration>
            <max_linear_velocity>1</max_linear_velocity>
            <min_linear_velocity>-1</min_linear_velocity>
            <max_angular_velocity>2</max_angular_velocity>
            <min_angular_velocity>-2</min_angular_velocity>
            <!-- <topic>/cmd_vel</topic> -->
        </plugin>



    </model> 
      
</sdf>